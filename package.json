{
  "name": "neuro-server",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start:nx": "nx serve",
    "start:debug": "nx run server:serve",
    "start:prod": "node dist/apps/server/main.js",
    "build": "nx build",
    "build:production": "nx build --prod",
    "test": "nx test",
    "test:cov": "nx run-many --target=test --all",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e --runInBand",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint --all",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "typeorm": "ts-node -P apps/server/tsconfig.migrations.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ormconfig.ts",
    "typeorm:create": "npm run typeorm migration:create -- -n",
    "typeorm:generate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "export PRODUCTION=true || set PRODUCTION=true&& npm run typeorm migration:run",
    "tool:compile-migrations": "tsc --project apps/server/tsconfig.migrations.json",
    "tool:prepare-templates": "ts-node --project=tools/tsconfig.tools.json tools/prepare-templates.ts",
    "tool:create-entities-index": "ts-node --project=tools/tsconfig.tools.json tools/create-entities-index.ts",
    "tool:merge-coverage": "ts-node --pretty --project=tools/tsconfig.tools.json tools/merge-coverage.ts",
    "tool:check-imports": "ts-node --pretty --project=tools/tsconfig.tools.json tools/check-dependencies.ts",
    "tool:extract-entrypoints": "ts-node --pretty --project=tools/tsconfig.tools.json tools/extract-endpoints.ts",
    "tool:prepare-configuration": "ts-node --pretty --project=tools/tsconfig.tools.json tools/prepare-configuration.ts",
    "tool:generate-graphs": "ts-node --pretty --project=tools/tsconfig.tools.json tools/generate-graphs.ts",
    "postinstall": "npm run tool:prepare-templates && npm run tool:create-entities-index && npm run tool:extract-entrypoints && npm run tool:prepare-configuration && npm run tool:compile-migrations",
    "nest-lib": "nx generate @stechy1/schematics:nest-lib stim-lib-name -d",
    "doc": "npx @compodoc/compodoc -p apps/server/tsconfig.doc.json -n \"Neuro-stimulator server documentation\" --includes additional-documentation",
    "doc-watch": "npx @compodoc/compodoc -p apps/server/tsconfig.doc.json -n \"Neuro-stimulator server documentation\" --includes additional-documentation -s -w"
  },
  "private": true,
  "dependencies": {
    "@nestjs/class-transformer": "^0.4.0",
    "@nestjs/class-validator": "^0.13.4",
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.4",
    "@nestjs/cqrs": "^8.0.3",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/platform-socket.io": "^8.4.4",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/typeorm": "^8.0.3",
    "@nestjs/websockets": "^8.4.4",
    "@serialport/parser-delimiter": "10.3.0",
    "@stechy1/class-transformer-validator": "0.9.5",
    "@stechy1/diplomka-share": "1.0.55",
    "accesscontrol": "2.2.1",
    "bcrypt": "5.0.1",
    "cookie-parser": "1.4.6",
    "date-fns": "2.28.0",
    "image-size": "1.0.1",
    "jsonwebtoken": "8.5.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "serialport": "10.4.0",
    "socket.io": "4.5.0",
    "sqlite3": "5.0.6",
    "ts-node": "10.7.0",
    "tslib": "2.4.0",
    "typeorm": "0.2.45",
    "uuid": "8.3.2",
    "winston": "3.7.2",
    "winston-daily-rotate-file": "4.6.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.19",
    "@golevelup/ts-jest": "0.3.3",
    "@nestjs/schematics": "^8.0.10",
    "@nestjs/testing": "^8.4.4",
    "@nrwl/cli": "14.0.5",
    "@nrwl/eslint-plugin-nx": "14.0.5",
    "@nrwl/jest": "14.0.5",
    "@nrwl/nest": "14.0.5",
    "@nrwl/node": "14.0.5",
    "@nrwl/workspace": "14.0.5",
    "@stechy1/schematics": "1.0.3",
    "@types/bcrypt": "5.0.0",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.13",
    "@types/is-ci": "3.0.0",
    "@types/istanbul-lib-coverage": "2.0.4",
    "@types/istanbul-lib-report": "3.0.0",
    "@types/istanbul-reports": "3.0.1",
    "@types/jest": "27.4.1",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "17.0.12",
    "@types/serialport": "8.0.2",
    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.18.0",
    "coveralls": "3.1.1",
    "dotenv": "14.3.2",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "eslint-plugin-import": "2.26.0",
    "fast-glob": "3.2.11",
    "fs-extra": "10.1.0",
    "istanbul-lib-coverage": "3.2.0",
    "istanbul-lib-report": "3.0.0",
    "istanbul-reports": "3.1.3",
    "jest": "27.5.1",
    "jest-matcher-utils": "27.5.1",
    "prettier": "2.6.2",
    "supertest": "6.2.3",
    "ts-jest": "27.1.4",
    "tsconfig-paths": "3.14.1",
    "tslint": "6.1.3",
    "typescript": "4.6.4",
    "nx": "14.0.5"
  },
  "peerDependencies": {
    "class-validator": "^0.13.2",
    "class-transformer": "^0.5.1"
  }
}
